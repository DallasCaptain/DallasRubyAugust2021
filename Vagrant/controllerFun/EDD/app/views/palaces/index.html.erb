<table>
    <tr><th>ID</th><th>Model</th><th>Price</th></tr>
    <% @palaces.each do |palace| %>
        <tr><td><%= palace.id %></td><td><%= palace.model %></td><td>$<%= palace.price %></td></tr>
    <% end %>
</table>

<% content_for :bottom do %>
    <h3>This is the end</h3>
<% end %>

<% content_for :top do %>
    <h3>This is the top</h3>
    <img src="<%= asset_path 'house.jpg' %>" alt="dog house">
<% end %>